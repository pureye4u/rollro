		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"firebase": "^10.11.0",
		"flatpickr": "^4.6.13",
		"svelte-flatpickr": "^3.3.5"
	}

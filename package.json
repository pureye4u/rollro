{
	"name": "rollro",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npm run prepare && npm run dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"ncp:build": "cd ncp/mapDirection && ./build.sh",
		"ncp:deploy": "cd ncp/mapDirection && ./deploy.sh"
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.18",
		"@smui/dialog": "^7.0.0-beta.18",
		"@smui/fab": "^7.0.0-beta.18",
		"@smui/icon-button": "^7.0.0-beta.18",
		"@smui/list": "^7.0.0-beta.18",
		"@smui/menu": "^7.0.0-beta.18",
		"@smui/radio": "^7.0.0-beta.18",
		"@smui/segmented-button": "^7.0.0-beta.18",
		"@smui/snackbar": "^7.0.0-beta.18",
		"@smui/switch": "^7.0.0-beta.18",
		"@smui/textfield": "^7.0.0-beta.18",
		"@smui/top-app-bar": "^7.0.0-beta.18",
		"@sveltejs/adapter-auto": "^3.0.0",
